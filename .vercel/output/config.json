{
	"version": 3,
	"routes": [
		{
			"src": "/404/",
			"headers": {
				"Location": "/404"
			},
			"status": 308
		},
		{
			"src": "/about/",
			"headers": {
				"Location": "/about"
			},
			"status": 308
		},
		{
			"src": "/archives/",
			"headers": {
				"Location": "/archives"
			},
			"status": 308
		},
		{
			"src": "/blog/([^/]+?)/",
			"headers": {
				"Location": "/blog/$1"
			},
			"status": 308
		},
		{
			"src": "/blog(?:\\/(.*?))?/",
			"headers": {
				"Location": "/blog/$1"
			},
			"status": 308
		},
		{
			"src": "/projects/",
			"headers": {
				"Location": "/projects"
			},
			"status": 308
		},
		{
			"src": "/search/",
			"headers": {
				"Location": "/search"
			},
			"status": 308
		},
		{
			"src": "/tags/([^/]+?)(?:\\/(.*?))?/",
			"headers": {
				"Location": "/tags/$1/$2"
			},
			"status": 308
		},
		{
			"src": "/tags/",
			"headers": {
				"Location": "/tags"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404$",
			"dest": "_render"
		},
		{
			"src": "^\\/about$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects$",
			"dest": "_render"
		},
		{
			"src": "^\\/robots\\.txt$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml$",
			"dest": "_render"
		},
		{
			"src": "^\\/tags$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}